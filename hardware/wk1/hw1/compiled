L_0x1cf1480 .functor BUFZ 16, v0x1cf0570_0, C4<0000000000000000>, C4<0000000000000000>, C4<0000000000000000>;
v0x1cf0200_0 .net "acc", 0 0, v0x1cf0a20_0;  1 drivers
v0x1cf02e0_0 .net "clk", 0 0, v0x1cf0ae0_0;  1 drivers
v0x1cf03a0_0 .net/s "in", 7 0, v0x1cf0c80_0;  1 drivers
v0x1cf0490_0 .net/s "out", 15 0, L_0x1cf1480;  alias, 1 drivers
v0x1cf0570_0 .var/s "psum_q", 15 0;
v0x1cf06a0_0 .net "relu", 0 0, v0x1cf10e0_0;  1 drivers

    %delay 1, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x1cf0ae0_0, 0, 1;
    %delay 1, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1cf0ae0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1cf1240_0, 0, 1;

 .timescale 0 0;
v0x1ccb840_0 .var "dec2bin", 7 0;
v0x1cefc40_0 .var/i "weight", 31 0;
TD_sfp_tb.dec2bin ;
    %pushi/vec4 4294967295, 0, 32;
    %load/vec4 v0x1cefc40_0;
    %cmp/s;
    %jmp/0xz  T_0.0, 5;
    %pushi/vec4 0, 0, 1;
    %ix/load 4, 7, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x1ccb840_0, 4, 1;
    %jmp T_0.1;
T_0.0 ;
    %pushi/vec4 1, 0, 1;
    %ix/load 4, 7, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x1ccb840_0, 4, 1;
    %load/vec4 v0x1cefc40_0;
    %addi 128, 0, 32;
    %store/vec4 v0x1cefc40_0, 0, 32;
T_0.1 ;
    %pushi/vec4 6, 0, 32;
    %store/vec4 v0x1cf13e0_0, 0, 32;
T_0.2 ;
    %pushi/vec4 4294967295, 0, 32;
    %load/vec4 v0x1cf13e0_0;
    %cmp/s;
    %jmp/0xz T_0.3, 5;
    %pushi/vec4 2, 0, 32;
    %load/vec4 v0x1cf13e0_0;
    %pow/s;
    %subi 1, 0, 32;
    %load/vec4 v0x1cefc40_0;
    %cmp/s;
    %jmp/0xz  T_0.4, 5;
    %pushi/vec4 1, 0, 1;
    %ix/getv/s 4, v0x1cf13e0_0;
    %store/vec4 v0x1ccb840_0, 4, 1;
    %load/vec4 v0x1cefc40_0;
    %pushi/vec4 2, 0, 32;
    %load/vec4 v0x1cf13e0_0;
    %pow/s;
    %sub;
    %store/vec4 v0x1cefc40_0, 0, 32;
    %jmp T_0.5;
T_0.4 ;
    %pushi/vec4 0, 0, 1;
    %ix/getv/s 4, v0x1cf13e0_0;
    %store/vec4 v0x1ccb840_0, 4, 1;
T_0.5 ;
    %load/vec4 v0x1cf13e0_0;
    %subi 1, 0, 32;
    %store/vec4 v0x1cf13e0_0, 0, 32;
    %jmp T_0.2;
T_0.3 ;
    %end;
S_0x1cefd20 .scope module, "sfp_instance" "sfp" 2 50, 3 3 0, S_0x1cc26f0;
 .timescale 0 0;
    .port_info 0 /OUTPUT 16 "out"

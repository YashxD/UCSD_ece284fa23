    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x1cf0bb0_0, 0, 32;
T_3.0 ;
    %load/vec4 v0x1cf0bb0_0;
    %cmpi/s 10, 0, 32;
    %jmp/0xz T_3.1, 5;
    %delay 1, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x1cf0ae0_0, 0, 1;
    %delay 1, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1cf0ae0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x1cf0a20_0, 0, 1;
    %pushi/vec4 64, 0, 16;
    %store/vec4 v0x1cf1310_0, 0, 16;
    %vpi_func 2 81 "$fscanf" 32, v0x1cf0e60_0, "%d\012", v0x1cf0d50_0 {0 0 0};
    %store/vec4 v0x1cf0f40_0, 0, 32;
    %load/vec4 v0x1cf0d50_0;
    %store/vec4 v0x1cefc40_0, 0, 32;
    %fork TD_sfp_tb.dec2bin, S_0x1ccb670;

v0x1cf0820_0 .net/s "thres", 15 0, v0x1cf1310_0;  1 drivers
E_0x1cf01a0 .event posedge, v0x1cf02e0_0;
    .scope S_0x1cefd20;
T_1 ;
    %wait E_0x1cf01a0;
    %load/vec4 v0x1cf0760_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 16;
    %store/vec4 v0x1cf0570_0, 0, 16;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x1cf0200_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_1.2, 4;
    %load/vec4 v0x1cf0570_0;
    %load/vec4 v0x1cf03a0_0;
    %pad/s 16;
    %add;
    %store/vec4 v0x1cf0570_0, 0, 16;
    %jmp T_1.3;
T_1.2 ;
    %load/vec4 v0x1cf06a0_0;
    %pad/u 32;
    %pushi/vec4 1, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x1cf0570_0;
    %load/vec4 v0x1cf0820_0;
    %cmp/s;
    %flag_get/vec4 5;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.4, 8;
    %pushi/vec4 0, 0, 16;
    %store/vec4 v0x1cf0570_0, 0, 16;
T_1.4 ;
T_1.3 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x1cc26f0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1cf0ae0_0, 0, 1;
